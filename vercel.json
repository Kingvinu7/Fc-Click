{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node@2.10.3"  // <--- Changed this line! Explicitly define Node.js version.
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
